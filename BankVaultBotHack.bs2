'END OF LOOP 3

PAUSE 1000

'LOOP4

FOR LOOP4 = 1 TO 4

IF LOOP4 = VAULT4 THEN

DEBUG " FOURTH NUMBER IS:" , DEC LOOP4

'SEND SIGNAL TO PIN ACCORDING TO BANK CODE DIGIT

IF LOOP4 = 1 THEN PULSOUT PIN0, 2000
